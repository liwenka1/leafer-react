{
	"name": "leafer-react",
	"version": "1.0.0",
	"description": "React bindings for Leafer UI",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf dist"
	},
	"dependencies": {
		"leafer-ui": "^1.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.0.0",
		"fs-extra": "^11.3.0",
		"kolorist": "^1.8.0",
		"rimraf": "^5.0.0",
		"tsup": "^7.0.0",
		"typescript": "^5.0.0"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"leafer-ui": "^1.0.0"
	}
}
